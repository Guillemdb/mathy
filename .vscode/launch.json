{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "mathy:main",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "main",
      "args": [
        "mixed",
        "agents/ablated",
        "-d",
        "4"
      ]
    },
    {
      "name": "mathy:to_features",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "to_features",
      "args": [
        "agents/mt_07_15_19",
        "4x + 2x",
        "2x + 4x"
      ]
    },
    {
      "name": "mathy:evaluate",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "evaluate"
    },
    {
      "name": "mathy:runner",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "runner",
      "args": [
        "training/dev/",
        "-l",
        "dev"
      ]
    },
    {
      "name": "mathy:lstm",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "lstm"
    },
    {
      "name": "mathy:train",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "train",
      "args": [
        "training/mathy_micro_dev/",
        "training.jsonl"
      ]
    },
    {
      "name": "mathy:r2d2",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "r2d2",
      "args": [
        "poly-grouping",
        "training/dev-r2d2",
        "-workers",
        "1"
      ]
    },
    {
      "name": "mathy:a3c",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "a3c",
      "args": [
        "poly-grouping",
        "training/dev",
        "-workers",
        "1"
      ]
    },
    {
      "name": "mathy:tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--cov=mathy"
      ]
    },
    {
      "name": "mathy:tests_filtered",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--pyargs",
        "mathy",
        "-k",
        "test_distributive_factor_out"
      ]
    },
    {
      "name": "docs:tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--cov=mathy",
        "--pyargs",
        "docs",
      ]
    },
    {
      "name": "docs:tests_filtered",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--pyargs",
        "docs",
        "-k",
        "test_distributive_factor_out"
      ]
    },
    {
      "name": "python:current_file",
      "type": "python",
      "justMyCode": false,
      "request": "launch",
      "program": "${file}"
    }
  ]
}
