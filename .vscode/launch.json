{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MathZero (Train)",
      "type": "python",
      "request": "launch",
      "debugStdLib": true,
      "pythonPath": "${config:python.pythonPath}",
      "module": "train",
      "args": [
        "/mnt/gcs/mzc/mathy_alpha_resnet_fix/",
        "/mnt/gcs/mzc/mathy_alpha/train/examples.jsonl"
      ]
    },
    {
      "name": "MathZero (Dev Train)",
      "type": "python",
      "request": "launch",
      "debugStdLib": true,
      "pythonPath": "${config:python.pythonPath}",
      "module": "main",
      "args": [
        "/mnt/gcs/mzc/seq_predictions/",
        "-v"
      ]
    },
    {
      "name": "math_agent (sac)",
      "type": "python",
      "request": "launch",
      "pythonPath": "${workspaceFolder}/.env/bin/python",
      "module": "sac",
      "args": [
        "--root_dir=.training/debug/",
        "--model_dir=/mnt/gcs/mzc/mathy_4/"
      ]
    },
    {
      "name": "math_agent (dqn)",
      "type": "python",
      "request": "launch",
      "module": "dqn",
      "args": [
        "--root_dir=.training/dqn/"
      ]
    },
    {
      "name": "Math Model Tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--cov=mathzero",
        "--pyargs",
        "mathzero"
      ]
    },
    {
      "name": "Math Tests (Focused)",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--pyargs",
        "mathzero",
        "-k",
        "test_math_game_jd"
      ]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}"
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "${workspaceFolder}",
      "port": 3000,
      "secret": "my_secret",
      "host": "localhost"
    }
  ]
}
