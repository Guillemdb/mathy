{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "mathy:evaluate",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "evaluate"
    },
    {
      "name": "mathy:zero",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "self_play",
      "args": [
        "training/dev",
        "-workers=5"
      ]
    },
    {
      "name": "mathy:a3c",
      "type": "python",
      "request": "launch",
      "justMyCode": false,
      "module": "a3c",
      "args": [
        "poly",
        "training/dev",
        "-workers=1",
        "-show",
        "-difficulty=easy"
      ]
    },
    {
      "name": "mathy:tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--cov=mathy"
      ]
    },
    {
      "name": "mathy:tests_filtered",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--pyargs",
        "mathy",
        "-k",
        "test_adr"
        // "test_episode_memory"
        // "test_parser_to_string"
        // "test_mathy_features_hierarchy"
        // "test_associative_property"
        // "test_commutative_property"
      ]
    },
    {
      "name": "docs:tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--cov=mathy",
        "--pyargs",
        "docs",
      ]
    },
    {
      "name": "docs:tests_filtered",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--pyargs",
        "docs",
        "-k",
        "test_distributive_factor_out"
      ]
    },
    {
      "name": "python:current_file",
      "type": "python",
      "justMyCode": false,
      "request": "launch",
      "program": "${file}"
    }
  ]
}
