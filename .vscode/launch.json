{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MathZero (Train)",
      "type": "python",
      "request": "launch",
      "module": "train",
      "args": [
        "/mnt/gcs/mzc/train_mathy_focus_one_tower_small/",
        "/mnt/gcs/mzc/fixed/focus_actions/examples.jsonl",
        "-n"
      ]
    },
    {
      "name": "MathZero (Dev Train)",
      "type": "python",
      "request": "launch",
      "module": "main",
      "args": ["/mnt/gcs/mzc/dev/", "-v"]
    },
    {
      "name": "math_agent (sac)",
      "type": "python",
      "request": "launch",
      "pythonPath": "${workspaceFolder}/.env/bin/python",
      "module": "sac",
      "args": [
        "--root_dir=.training/debug/",
        "--model_dir=/mnt/gcs/mzc/mathy_4/"
      ]
    },
    {
      "name": "math_agent (dqn)",
      "type": "python",
      "request": "launch",
      "module": "dqn",
      "args": ["--root_dir=.training/dqn/"]
    },
    {
      "name": "Math Model Tests",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["--cov=mathzero", "--pyargs", "mathzero"]
    },
    {
      "name": "Math Tests (Focused)",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": ["--pyargs", "mathzero", "-k", "test_math_game_jd"]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}"
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "${workspaceFolder}",
      "port": 3000,
      "secret": "my_secret",
      "host": "localhost"
    }
  ]
}
